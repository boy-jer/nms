<h1>Listing iklans</h1>

<table>
  <tr>
    <th>Invoice</th>
    <th>Entry name</th>
    <th>Customer name</th>
    <th>Input date</th>
    <th>Publish date</th>
    <th>Type</th>
    <th>Category</th>
    <th>Segmentation</th>
    <th>Content</th>
    <th>Character</th>
    <th>Discount</th>
    <th>Price</th>
    <th>Session</th>
    <th>Confirmed</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @iklans.each do |iklan| %>
  <tr>
    <td><%= iklan.invoice %></td>
    <td><%= iklan.entry_name %></td>
    <td><%= iklan.customer_name %></td>
    <td><%= iklan.input_date %></td>
    <td><%= iklan.publish_date %></td>
    <td><%= iklan.type %></td>
    <td><%= iklan.category %></td>
    <td><%= iklan.segmentation %></td>
    <td><%= iklan.content %></td>
    <td><%= iklan.character %></td>
    <td><%= iklan.discount %></td>
    <td><%= iklan.price %></td>
    <td><%= iklan.session %></td>
    <td><%= iklan.confirmed %></td>
    <td><%= link_to 'Show', iklan %></td>
    <td><%= link_to 'Edit', edit_iklan_path(iklan) %></td>
    <td><%= link_to 'Destroy', iklan, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Iklan', new_iklan_path %>
